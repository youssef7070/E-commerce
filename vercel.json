{
    "builds": [
        {
            "src": "dist/Ecommerce/browser/**",
            "use": "@vercel/static"
        },
        {
            "src": "dist/Ecommerce/server/**",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/Ecommerce/server/main.js"
        }
    ]
}